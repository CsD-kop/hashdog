{
  "name": "hashdog",
  "version": "1.2.12",
  "description": "Break MD5/SHA1/SHA256/SHA512 hashes using wordlists, password lists and string permutations",
  "repository": {
    "type": "git",
    "url": "https://github.com/logotype/hashdog.git"
  },
  "license": "MIT",
  "main": "src",
  "bin": {
    "hashdog": "./bin/hashdog-cli.js"
  },
  "dependencies": {
    "commander": "latest",
    "colors": "latest",
    "tar": "latest"
  },
  "devDependencies": {
    "del": "latest",
    "gulp": "latest",
    "gulp-cli": "latest",
    "babel": "latest",
    "gulp-chmod": "latest",
    "gulp-babel": "latest",
    "gulp-mocha": "latest",
    "gulp-jshint": "latest",
    "gulp-rename": "latest",
    "gulp-util": "latest",
    "mocha": "latest",
    "gulp-load-plugins": "latest"
  },
  "keywords": [
    "MD5",
    "SHA1",
    "SHA256",
    "SHA512",
    "hashdog",
    "bruteforce",
    "ES6"
  ],
  "scripts": {
    "test": "mocha --compilers js:babel/register test/**/*.js",
    "perf": "./node_modules/gulp-cli/bin/gulp.js --silent; ./node_modules/gulp-cli/bin/gulp.js perf --silent; iojs perfbuild/Test-Hashers.js"
  }
}